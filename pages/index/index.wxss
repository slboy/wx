/* 10-31 gp  */
.container{
  background: #F5F5F5;
  position: relative;
  padding-bottom: 98rpx;
  box-sizing: border-box;
  height: 100%;
}
/*public  */
.title{
  font-size: 44rpx;
  text-align: center;
  position: relative;
  margin-bottom: 80rpx;
}
.title:after{
  content: '';
  width: 70rpx;
  height: 2px;
  position: absolute;
  left: 0;right: 0;bottom: -20rpx;
  margin: auto;
  background: #D30000;
}
/* loading  */
.loading_box {
  width: 100%;
  text-align: center;
  padding: 30rpx 0;
  display: none;
}
.loading_box.none{
  display: block;
}
.loading_box>view {
  display: inline-block;
}
.loading_fade_circle {
  width: 30rpx;
  height: 30rpx;
  background: #1ab394;
  border-radius: 50%;
  -webkit-animation: fading_circle 1s ease-out infinite alternate;
  animation: fading_circle 1s ease-out infinite alternate;
}

@-webkit-keyframes fading_circle {
  0% {
    transform: scale(1.5);
    opacity: 0.2;
  }

  100% {
    transform: scale(0.1);
    opacity: 1;
  }
}

@keyframes fading_circle {
  0% {
    transform: scale(1.5);
    opacity: 0.2;
  }

  100% {
    transform: scale(0.1);
    opacity: 1;
  }
}
/* partone */
.logo{
  padding-top: 80rpx;
  background: #fff;
  padding-bottom: 60rpx;
}
.logo_text_tit{
  font-size: 48rpx;
}
.logo_text_tit:first-child{
  padding-left: 135rpx;
  margin-bottom: 25rpx;
}
.logo_text_tit:last-child{
  padding-left: 180rpx;
}
.logo_text_tit text{
  color: #D30000;
}
.logo_pic{
  display: flex;
  justify-content:center;
  margin-top: 40rpx;
}
.logo_logo{
  margin-right: 80rpx;
}
.logo_logo image{
  width: 190rpx;
  height: 96rpx;
}
.logo_item{
  font-size: 24rpx;
}
.logo_item image{
  margin-right: 10rpx;
  vertical-align: top;
  position: relative;
  top: 4rpx;
}
.logo_item:first-child{
  margin-bottom: 30rpx;
}
.logo_item:first-child image{
  width: 26rpx;
  height: 26rpx;
}
.logo_item:last-child image{
  padding-left: 4rpx;
  margin-right: 12rpx;
  width: 20rpx;
  height: 24rpx;
}
/*partTwo  */
.cando{
  padding-top: 55rpx;
}
.cando_item{
  font-size: 0;
  padding-left: 76rpx;
  margin-bottom: 48rpx;
}
.cando_item image{
  width: 114rpx;
  height: 114rpx;
  margin-right: 40rpx;
}
.cando_item text{
  line-height: 114rpx;
  display: inline-block;
  vertical-align: top;
  font-size: 28rpx;
}
/*partThree  */
.case{
  padding-top: 20rpx;
  position: relative;
}
.case_list{
  text-align: center;
  height: 580rpx;
  overflow: hidden;
  position: relative;
}
.case:after{
  content: '';
  position: absolute;
  bottom: 0;left: 0;
  width: 100%;
  height: 96rpx;
  background: linear-gradient(to bottom, rgba(255,255,255,0), rgba(245,245,245,1));
}
.case_item{
  font-size: 0;
  width: 320rpx;
  padding-bottom: 20rpx;
  display: inline-block;
  vertical-align: top;
}
.case_item:nth-child(odd){
  margin-right: 30rpx;
}
.case_item image{
  width: 320rpx;
  height: 180rpx;
}
.case_item text{
  display: block;
  font-size: 24rpx;
  margin-top: 20rpx;
  text-align: center;
}
/*partFour  */
.who{
  padding-top: 50rpx;
}
.who_list{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.who_item{
  width: 50%;
  text-align: center;
  font-size: 0;
  margin-bottom: 40rpx;
}
.who_item image{
  width: 114rpx;
  height: 114rpx;
  margin-bottom: 25rpx;
}
.who_item .name{
  font-size: 24rpx;
  color: #4d4d4d;
  margin-bottom: 20rpx;
  font-weight: bold;
}
.who_item .text{
  font-size: 20rpx;
  color: #666;
  line-height: 30rpx;
}
.who_item:nth-child(3) .text view:last-child{
  color: #999;
}
.who_item:nth-child(4) .text view:last-child{
  color: #999;
}
/*partFive  */
.serve{
  padding-top: 15rpx;
}
.serve_list{
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.serve_item{
  width: 50%;
  margin-bottom: 60rpx;
  font-size: 0;
  box-sizing: border-box;
}
.serve_item:nth-child(odd){
  padding-left: 76rpx;
}
.serve_item:nth-child(even){
  padding-left: 10rpx;
}
.serve_item:nth-child(1) image{
  width: 215rpx;
  height: 43rpx;
}
.serve_item:nth-child(2) image{
  width: 279rpx;
  height: 45rpx;
}
.serve_item:nth-child(3) image{
  width: 200rpx;
  height: 44rpx;
}
.serve_item:nth-child(4) image{
  width: 192rpx;
  height: 47rpx;
}
.serve_item text{
  margin-top: 20rpx;
  display: block;
  font-size: 20rpx;
  color: #808080;
}
/*partSix  */
.demand{
  padding-top: 30rpx;
  padding-bottom: 44rpx;
}
.form_box{
  background: #fff;
  width: 670rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  margin-bottom: 35rpx;
}
.form_item:first-child{
  border-bottom: 1px solid #EEEEEE;
}
.form_item{
  line-height: 100rpx;
  padding-left: 30rpx;
  font-size: 36rpx;
  color: #333;
  box-sizing: border-box;
}
.form_item label{
  display: block;
}
.form_item text{
  margin-right: 90rpx;
}
.form_item .picker{
  display: inline-block;
}
.form_item .tit{
  display: inline-block;
  color: #999;
}
.form_item .inp{
  display: inline-block;
  vertical-align: middle;
  line-height: 108rpx;
  position: relative;
  top: -4rpx;
}
.form_item .inp input{
  width: 380rpx;
}
.demand button{
  line-height: 108rpx;
  width: 670rpx;
  background: #D30000;
  color: #fff;
  font-size: 36rpx;
}
/*partSeven  */
.contact{
  position: fixed;
  bottom: 0;left: 0;
  height: 98rpx;
  width: 100%;
  font-size: 0;
  z-index: 10000;
}
.contact button{
  width: 50%;
  display: inline-block;
  border-radius: 0;
  line-height: 98rpx;
  font-size: 32rpx;
}
.contact button::after{
  display: none;
}
.contact button:first-child{
  background: #fff;
  color: #D30000;
}
.contact button:last-child{
  background: #D30000;
  color: #fff;
}
/* //错误框 */
.modal{
  position: absolute;
  left: 0;top: 0;
  width: 100%;
  z-index: 1000000;
  height: 100%;
  display: none;
}
.modal.none{
  display: block;
}
.modal_mask{
  width: 100%;
  height: 100%;
  background: rgba(178,178,178,0);
}
.modal_body{
  box-sizing: border-box;
  width: 240rpx;
  line-height: 50rpx;
  height: 200rpx;
  padding: 50rpx 20rpx;
  border-radius: 10rpx;
  font-size: 40rpx;
  color: #fff;
  position: fixed;
  left: 0;right: 0;
  top: 0;bottom: 0;
  margin: auto;
  text-align: center;
  background: rgba(66,66,66,0.8);
}